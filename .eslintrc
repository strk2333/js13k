{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es6": true
  },
  "rules": {
    "brace-style": [
      2,
      "1tbs"
    ],
    "quotes": [
      2,
      "single",
      "avoid-escape"
    ],
    "curly": 0,
    "eqeqeq": [
      2,
      "smart"
    ],
    "no-floating-decimal": 2,
    "no-comma-dangle": 0,
    "no-use-before-define": [
      2,
      "nofunc"
    ],
    "strict": 0
  }
}
